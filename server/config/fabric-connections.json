{
  "fabric-connections": {
    "serviceDiscovery": {
      "enabled": true,
      "asLocalhost": false
    },
    "conn1": [
      {
        "channel": "defaultchannel",
        "chaincodes": [
          "pingcc"
        ]
      }
    ]
  },
  "routes": [
    {
      "path": "/ping",
      "fabric-connection": "conn1",
      "modulePath": "routes/ping"
    },
    {
      "path": "/securedPing",
      "fabric-connection": "conn1",
      "modulePath": "routes/securedPing", 
      "protected" : {
        "enabled": true, 
        "allowedClients": [
          ""
        ]
      }
    }
  ]
}
