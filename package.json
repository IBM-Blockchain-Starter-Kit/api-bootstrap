{
  "name": "app",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "NODE_CONFIG_DIR=./server/config tsc",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/server.js' --watch '.server/dist'",
    "start": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "dev": "NODE_CONFIG_DIR=./server/config ts-node-dev server/server.ts",
    "lint": "eslint . --ext ts",
    "lintfix": "eslint --fix .",
    "test": "FABRIC_ENROLL_ID=test FABRIC_ENROLL_SECRET=testpw NODE_CONFIG_DIR=./test/mocks/config nyc mocha --recursive --exit"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "express": "~4.16.0",
    "fabric-ca-client": "1.4.0",
    "fabric-client": "1.4.0",
    "fabric-network": "1.4.0",
    "http-errors": "~1.6.2",
    "ibmcloud-appid": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.10",
    "log4js": "^4.3.1",
    "passport": "^0.4.0",
    "swagger-ui-express": "^4.0.2",
    "typescript": "^3.5.2",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/express": "^4.17.0",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/passport": "^1.0.0",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/yamljs": "^0.2.30",
    "@typescript-eslint/parser": "^1.11.0",
    "bluebird": "3.5.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-typescript": "^0.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "rimraf": "^2.6.3",
    "sinon": "^5.0.7",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript-eslint-parser": "^22.0.0"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**"
    ],
    "reporter": [
      "lcov",
      "json",
      "text",
      "text-summary",
      "cobertura"
    ],
    "all": true
  }
}
